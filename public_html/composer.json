{
    "config": {
        "vendor-dir": "vendor",
        "allow-plugins": {
            "robloach/component-installer": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "3.6.1",
                "source": {
                    "url": "https://github.com/jquery/jquery.git",
                    "type": "git",
                    "reference": "3.6.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "danielm/uploader",
                "version": "v1.0.2",
                "source": {
                    "url": "https://github.com/danielm/uploader.git",
                    "type": "git",
                    "reference": "v1.0.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "joequery/stupid-table-plugin",
                "version": "1.1.3",
                "source": {
                    "url": "https://github.com/joequery/stupid-table-plugin.git",
                    "type": "git",
                    "reference": "1.1.3"
                }
            }
        }
    ],
    "require": {
        "twbs/bootstrap": "^5.2",
        "jquery/jquery": "3.6.1",
        "fortawesome/font-awesome": "^6.2",
        "joequery/stupid-table-plugin": "1.1.3"
    }
}